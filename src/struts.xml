<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <package name="Register" namespace="/action" extends="struts-default">
        <action name="login" class="com.action.RegisterAction" method="execute">
            <result name="success">/WEB-INF/result/success.jsp</result>
            <result name="fail">/WEB-INF/result/fail.jsp</result>
        </action>
    </package>

    <package name="Login" namespace="/action" extends="struts-default">
        <action name="login" class="com.action.LoginActin" method="execute">
            <result name="success">/WEB-INF/result/success.jsp</result>
            <result name="fail">/WEB-INF/result/fail.jsp</result>
        </action>
    </package>
</struts>